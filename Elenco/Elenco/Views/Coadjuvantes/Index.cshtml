@model IEnumerable<Elenco.Models.Coadjuvante>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nome)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Descricao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Retrato)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Origem)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ExperienciaAlfa)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ExperienciaBeta)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AutenticidadeAlfa)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AutenticidadeBeta)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AutenticidadeGama)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Descricao)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Retrato)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Origem.Nome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ExperienciaAlfa.Nome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ExperienciaBeta.Nome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AutenticidadeAlfa.Nome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AutenticidadeBeta.Nome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AutenticidadeGama.Nome)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
